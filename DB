                if v:IsA("Model") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChildWhichIsA("Humanoid") and v:FindFirstChildWhichIsA("Humanoid").Health > 0 and v.Name == "Jack'O Seijin" then
                    local distance = (rootPart.Position - v:WaitForChild("HumanoidRootPart").Position).Magnitude
                    if distance < nearestDistance then
                        nearestDistance, nearestEventMob = distance, v
                    end
                end
            end
